<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhuani21.review.dao.LoginAuthDao">
	<insert id="insertLoginAuth">
		insert into login_auth(user_id,username,password) values(#{user_id},#{username},#{password})
	</insert>
	<select id="findUserIdViaUsernamePassword" parameterType="com.zhuani21.review.bean.LoginAuth" resultType="int">
		select user_id from login_auth where username=#{username} and password=#{password}
	</select>
</mapper>